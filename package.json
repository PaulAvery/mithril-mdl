{
  "name": "mithril-mdl",
  "version": "1.1.0",
  "author": "Florian Albertz",
  "license": "BSD-3-Clause",
  "main": "lib/mdl.js",
  "description": "Material Design Lite components via mithril.js",
  "keywords": [
    "mithril",
    "mithril.js",
    "mdl",
    "material design",
    "material design lite"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulAvery/mithril-mdl.git"
  },
  "dependencies": {
    "mithril": "lhorie/mithril.js#next"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-plugin-transform-react-jsx": "^6.0.18",
    "babel-preset-es2015": "^6.1.2",
    "eslint": "^1.5.1",
    "eslint-plugin-react": "^3.6.3"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "plugins": [
      "react"
    ],
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "rules": {
      "curly": [
        2,
        "multi-line"
      ],
      "quotes": [
        2,
        "single"
      ],
      "react/jsx-uses-react": [
        2,
        {
          "pragma": "m"
        }
      ]
    }
  }
}
